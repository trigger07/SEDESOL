@using SEDESOL.DataEntities.DTO;
@model List<GencodeDayDTO>

<table id="datesList" class="table table-striped table-bordered nowrap" width="100%">
    <thead>
        <tr>
            <th>Año</th>
            <th>Mes</th>
            <th>Día Inicio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="odd gradeX">
                <td>
                    @Html.DisplayFor(modelItem => item.YearDto.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MonthDto.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Day)
                </td>
            </tr>
        }

    </tbody>
</table>
<hr />
<div class="row">
    <div class="col-md-2" style="margin-top:10px;">
        <button type="button" class="btn btn-sm col-md-12  btn-danger" style="font-size:16px;" data-dismiss="modal" aria-hidden="true">Cerrar</button>
    </div>

</div>

