@using SEDESOL.DataEntities.DTO;
@model List<SoupKitchenDTO>

<table id="skAssignedList" class="table table-striped table-bordered nowrap" width="100%">
    <thead>
        <tr>
            <th>Comedor</th>
            <th>Ubicación</th>
            
            <th>Contacto</th>
            <th>Teléfono</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="odd gradeX">
                <td onclick="QuitSk(@item.Id)" style="cursor: pointer;" title="Quitar comedor">
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td onclick="QuitSk(@item.Id)" style="cursor: pointer;" title="Quitar comedor">
                    @Html.DisplayFor(modelItem => item.State.Name)
                </td>
                
                <td onclick="QuitSk(@item.Id)" style="cursor: pointer;" title="Quitar comedor">
                    @Html.DisplayFor(modelItem => item.ContactName)
                </td>
                <td onclick="QuitSk(@item.Id)" style="cursor: pointer;" title="Quitar comedor">
                    @Html.DisplayFor(modelItem => item.PhoneNumber)
                </td>
            </tr>
        }
    </tbody>
</table>

